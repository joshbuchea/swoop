{
	"name": "Swoop!",
	"version": "1.0",
	"description": "Tab navigation done right.",
	"manifest_version": 2,
	"background": {
		"scripts": ["event-page/fuse.min.js", "event-page/index.js"],
		"persistent": true
	},
	"permissions": ["tabs", "<all_urls>"],
	"commands": {
			"toggle": {
				"suggested_key": {
					"default": "Ctrl+Shift+K",
					"mac": "Command+Shift+K"
				},
				"description": "Toggle switcher UI"
			}
		}
}
